Map {
  background-color: #fff;
}

#detroitborder2 {
  line-width:2;
  line-color:#000;
  line-opacity:.45;
  line-dasharray: 10, 5;
}


#detroit {
  line-width:1;
  line-color:#777;
  line-opacity:.25;
  line-dasharray: 2, 2, 2, 2;
}


#parkspolygon {
  line-color:#999;
  line-width:1;
  line-opacity:.55;
  polygon-opacity:.2;
  polygon-fill:#999;
}


#bunge1 {
  line-width:3;
  line-color:#333;
  line-opacity:.35;
  text-name: [Name];
  text-face-name: 'Droid Sans Regular';
  text-fill: #333;
  text-size: 15;
  text-opacity:.75;
  text-placement: line;
  text-dy: 10;
  text-max-char-angle-delta: 15;
}
